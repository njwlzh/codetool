<?xml version="1.0" encoding="UTF-8"?>
<configure>
	<basedir>F:/code/babyshop</basedir>
	<basePackage>com.mars</basePackage>
	<db>
		<dbName>babyshop_server</dbName>
		<user>babyshop</user>
		<pwd>babyshop</pwd>
		<driver>com.mysql.jdbc.Driver</driver>
		<url>jdbc:mysql://localhost:3306/babyshop_server?useUnicode=true&amp;characterEncoding=utf-8</url>
	</db>
	
	<ignoreColumns>id, com_id, com_code, create_user, create_time, update_user, update_time, state, note,extra_data, inst_id, sys_time</ignoreColumns>
	
	<packageSetting>
		<actionPackage>controller</actionPackage>
		<daoPackage>dao</daoPackage>
		<daoImplPackage>impl</daoImplPackage>
		<servicePackage>service</servicePackage>
		<serviceImplPackage>impl</serviceImplPackage>
		<entityPackage>entity</entityPackage>
		<mapperPackage>mapper</mapperPackage>
		<viewPackage>view</viewPackage>
		<isDeleteTablePrefix>true</isDeleteTablePrefix>
	</packageSetting>
